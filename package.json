{
  "name": "@vertexvis/vertex-cli",
  "version": "0.3.6",
  "description": "The Vertex platform command-line interface (CLI).",
  "license": "MIT",
  "author": "Vertex Developers <support@vertexvis.com> (https://developer.vertexvis.com)",
  "homepage": "https://github.com/Vertexvis/vertex-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vertexvis/vertex-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Vertexvis/vertex-cli/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "bin": {
    "vertex": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@vertexvis/vertex-api-client": "^0.6",
    "cli-ux": "^5.5",
    "fast-xml-parser": "^3.17",
    "nanoid": "^3.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^14.14",
    "@vertexvis/eslint-config-vertexvis-typescript": "0.3",
    "eslint": "^7",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "globby": "^11",
    "husky": "^4.3",
    "lint-staged": "^10.5",
    "prettier": "^2.2",
    "ts-node": "^9",
    "typescript": "^4.1"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "LICENSE"
  ],
  "keywords": [
    "oclif"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vertex",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf lib && rm -f oclif.manifest.json",
    "format": "prettier --write './**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|mdx|html|css)'",
    "lint": "eslint . --ext .ts --config .eslintrc --ignore-path .gitignore",
    "lint-staged": "yarn lint && yarn format",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn clean && yarn build && oclif-dev manifest && oclif-dev readme",
    "setup": "yarn && yarn link",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "yarn format"
    ]
  }
}
